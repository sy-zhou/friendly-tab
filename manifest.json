{
    "name": "Friends Tab",
    "description": "A silly tab made to cheer up all Friends geeks.",
    "version": "1.0",

    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["tabs"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "Images/central_perk_logo.png"
    },

    "manifest_version": 2
}
